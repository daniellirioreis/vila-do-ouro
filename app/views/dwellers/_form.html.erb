<%= simple_form_for(@dweller) do |f| %>
	<div class="row">
		<div class="col-lg-7">
			<%= f.input :name %>
		</div>
	</div>
	<div class="row">
		<div class="col-lg-5">
    	<%= f.input :email %>
		</div>
		<div class="col-lg-2">
    	<%= f.input :phone %>
		</div>		
		<div class="col-lg-3">
			<%= f.input :cpf %>
    </div>
  </div>

  <div class="form-actions">
	  <%= f.button :submit, class: "btn btn-success" %>
  </div>
<% end %>
